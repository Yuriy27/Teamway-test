services:
  workshifts.api:
    build:
      context: C:\Users\yukuc\source\repos\WorkShifts
      dockerfile: WorkShifts.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WorkShifts.Api
      target: base
    container_name: WorkShifts.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: workshiftsapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/WorkShifts.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\yukuc\source\repos\WorkShifts\WorkShifts.Api:/app:rw
    - C:\Users\yukuc\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\yukuc\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\yukuc\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\yukuc\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\yukuc\source\repos\WorkShifts:/src:rw
version: '3.4'